[
  {
    "swagger": "2.0",
    "info": {
      "version": "v1",
      "title": "Swagger Petstore",
      "license": {
        "name": "MIT"
      }
    },
    "basePath": "/api/v1/",
    "paths": {
      "/test": {
        "get": {
          "summary": "List all pets",
          "operationId": "operation1",
          "tags": ["pets"],
          "parameters": [
            {
              "name": "limit",
              "in": "query",
              "description": "How many items to return at one time (max 100)",
              "required": false,
              "type": "integer"
            }
          ],
          "produces": ["application/json"],
          "responses": {
            "200": {
              "description": "A paged array of pets",
              "schema": {
                "$ref": "#/components/schemas/Pets"
              }
            }
          }
        }
      }
    },
    "definitions": {
      "Pet": {
        "required": ["id", "name"],
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "name": {
            "type": "string"
          },
          "tag": {
            "type": "string"
          }
        }
      },
      "Pets": {
        "type": "array",
        "items": {
          "$ref": "#/components/schemas/Pet"
        }
      },
      "Error": {
        "required": ["code", "message"],
        "properties": {
          "code": {
            "type": "integer",
            "format": "int32"
          },
          "message": {
            "type": "string"
          }
        }
      }
    }
  }
]
