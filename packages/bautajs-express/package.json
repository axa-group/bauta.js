{
  "name": "@bautajs/express",
  "version": "2.0.0-alpha.1",
  "description": "A bautaJS express plugin. Allows auto exposing the bautaJS services.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/**/*.js",
    "./dist/**/*.d.ts",
    "./dist/index.d.ts"
  ],
  "scripts": {
    "example": "LOG_LEVEL=debug DEBUG=bautajs* node ./examples/server.js",
    "documentation": "typedoc --name @bautajs/express ./src",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.axa.com/Digital/bauta-nodejs"
  },
  "publishConfig": {
    "registry": "https://axags.jfrog.io/axags/api/npm/lib-amf-release"
  },
  "keywords": [
    "bautajs",
    "express",
    "middleware"
  ],
  "author": "javier.blanco@axa.com",
  "license": "SEE LICENSE IN LICENSE.txt",
  "dependencies": {
    "@bautajs/core": "2.0.0-alpha.1",
    "body-parser": "1.18.3",
    "chalk": "2.4.2",
    "compression": "1.7.3",
    "cors": "2.8.5",
    "express": "4.16.4",
    "helmet": "3.15.1",
    "morgan": "1.9.1",
    "openapi-types": "1.3.4",
    "route-order": "0.1.0",
    "swagger-ui-express": "4.0.2"
  }
}
