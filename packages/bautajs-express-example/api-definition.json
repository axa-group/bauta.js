    "components": {
      "schemas": {
        "ThisShouldNotBeIncluded": {
          "required": [
            "userId",
            "title"
          ],
          "properties": {
            "userId": {
              "type": "integer",
              "format": "int64"
            },
            "id": {
              "type": "integer",
              "format": "int64"
            },
            "title": {
              "type": "string"
            },
            "body": {
              "type": "string"
            }
          }
        },
        "Article": {
          "required": [
            "title"
          ],
          "properties": {
            "userId": {
              "nullable": true,
              "type": "integer",
              "format": "int64"
            },
            "id": {
              "type": "integer",
              "format": "int64"
            },
            "title": {
              "type": "string"
            },
            "body": {
              "type": "string"
            }
          }
        },
        "Articles": {
          "type": "array",
          "items": {
            "$ref": "#/components/schemas/Article"
          }
        },
        "Error": {
          "required": [
            "message"
          ],
          "properties": {
            "code": {
              "type": "integer",
              "format": "int32"
            },
            "message": {
              "type": "string"
            }
          }
        },
        "RequestMinimap": {
          "type": "object",
          "required": ["key", "value"],
          "description": "Request body for a minimap register",
          "properties": {
            "key": {
              "type": "string",
              "description": "The key of the minimap register"
            },
            "value": {
              "type": "string",
              "description": "The value of the minimap register"
            }
          }
        }
      }
    }
