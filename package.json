{
  "repository": {
    "type": "git",
    "url": "git+https://github.axa.com/Digital/bauta-nodejs.git"
  },
  "keywords": [
    "middleware",
    "express",
    "fastify",
    "agent",
    "proxy",
    "multipart"
  ],
  "author": "mit@axa-groupsolutions.com",
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://github.axa.com/Digital/bauta-nodejs/issues"
  },
  "homepage": "https://github.axa.com/Digital/bauta-nodejs#readme",
  "devDependencies": {
    "eslint": "6.0.1",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-prettier": "3.1.0",
    "jest": "24.8.0",
    "jest-extended": "0.11.2",
    "jest-junit": "6.4.0",
    "lerna": "3.15.0",
    "lint-staged": "9.2.0",
    "nock": "11.0.0-beta.23",
    "pre-commit": "1.2.2",
    "prettier": "1.18.2",
    "supertest": "4.0.2"
  },
  "scripts": {
    "lerna": "lerna",
    "eslint": "eslint",
    "bootstrap": "lerna bootstrap",
    "postinstall": "lerna bootstrap",
    "documentation": "lerna exec -- npm run documentation && git add '*/**/README.md'",
    "clean": "lerna clean --yes",
    "release": "npm run clean && npm i && npm run test && lerna publish --exact --conventional-commits --no-verify-access --no-verify-registry --yes",
    "test": "npm run lint && jest --verbose",
    "coverage": "npm test -- --coverage",
    "test:clean": "jest --clearCache",
    "test:watch": "jest --verbose --watchAll",
    "test:ci": "npm run coverage -- --ci --maxWorkers=2  --reporters=default --reporters=jest-junit",
    "lint": "eslint .",
    "checkDeps": "lerna exec --concurrency 14 -- npm outdated"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/?(*.)+(spec|test|e2e).js?(x)"
    ],
    "projects": [
      "<rootDir>/packages/*"
    ]
  },
  "pre-commit": [
    "test"
  ]
}
