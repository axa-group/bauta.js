    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.21.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-axa-digital-node-typescript": "3.0.3",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.2.1",
    "eslint-plugin-prettier": "3.3.1",
    "fast-safe-stringify": "2.0.7",
    "form-data": "4.0.0",
    "got": "11.8.2",
    "husky": "5.1.3",
    "jest": "26.6.3",
    "jest-config": "26.6.3",
    "jest-extended": "0.11.5",
    "jest-junit": "12.0.0",
    "nock": "13.0.11",
    "node-mocks-http": "1.10.1",
    "pino": "6.11.2",
    "prettier": "2.2.1",
    "prettier-check": "2.0.0",
    "stream-mock": "2.0.5",
    "supertest": "6.1.3",
    "ts-jest": "26.5.3",
    "typescript": "4.2.3"
  },
  "scripts": {
    "cm": "git-cz",
    "benchmark-pipeline": "node ./packages/bautajs-core/benchmark/pipeline-builder.js",
    "benchmark-core": "npx concurrently -k -s first \"node ./packages/bautajs-core/benchmark/simple.js\" \"npx autocannon -c 100 -d 5 -p 10 localhost:8080/v1/api/\"",
    "benchmark-express": "npx concurrently -k -s first \"node ./packages/bautajs-express/benchmark/simple.js\" \"npx autocannon -c 20 -d 5 -p 10 localhost:8080/v1/api/\"",
    "benchmark-fastify": "npx concurrently -k -s first \"node ./packages/bautajs-fastify/benchmark/simple.js\" \"npx autocannon -c 100 -d 5 -p 10 localhost:8080/v1/api/\"",
    "lint": "eslint '*/**/*.{ts,tsx}' ",
    "bootstrap": "npx lerna bootstrap --no-ci",
    "postinstall": "husky install && npx lerna bootstrap --no-ci && npm run compile",
    "clean": "npx lerna clean --yes && git clean -dfqX -- ./node_modules **/{dist,node_modules}/ ./packages/*/tsconfig*tsbuildinfo ./packages/*/package-lock.json ./coverage",
    "release": "npm run clean && npm i && npm run test && npx lerna publish --exact --conventional-commits --no-verify-access --no-verify-registry --yes",
    "test": "jest --verbose --detectOpenHandles --testTimeout=30000",
    "coverage": "npm test -- --coverage",
    "test:clean": "jest --clearCache --testTimeout=30000",
    "test:watch": "jest --verbose --watchAll --testTimeout=30000",
    "test:ci": "npm run coverage -- --ci --maxWorkers=2  --reporters=default --reporters=jest-junit --testTimeout=30000",
    "checkDeps": "npx lerna exec --concurrency 14 -- npm outdated",
    "compile": "tsc --build tsconfig.build.json",
    "compile:clean": "tsc --build tsconfig.build.json --clean",
    "watch": "tsc --build tsconfig.build.json --watch"
  },
  "engines": {
    "node": ">=10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "projects": [
      "<rootDir>/packages/*"
    ]
  },
  "dependencies": {}
}
